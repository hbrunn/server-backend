<odoo>

<record id="view_push_notification_registration_tree" model="ir.ui.view">
    <field name="model">push.notification.registration</field>
    <field name="arch" type="xml">
        <tree editable="top">
            <field name="registration_type" />
            <field name="identifier" />
            <field name="partner_id" />
        </tree>
    </field>
</record>

<record id="view_push_notification_registration_search" model="ir.ui.view">
    <field name="model">push.notification.registration</field>
    <field name="arch" type="xml">
        <search>
            <field name="registration_type" />
            <field name="identifier" />
            <field name="partner_id" />
        </search>
    </field>
</record>

<record id="action_push_notification_registration" model="ir.actions.act_window">
    <field name="name">Registrations</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">push.notification.registration</field>
    <field name="view_mode">tree</field>
</record>

</odoo>
